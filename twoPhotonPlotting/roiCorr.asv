function roiCorr(mov,startFrame,endFrame,varargin)

%% Set start and end frame 


%% Select roi 
[~, intens] = clicky(mov);

BEGIN_TC = 80;
END_TC = 400;

%% Perform correlation 
rho = corr(squeeze(intens(BEGIN_TC:END_TC,1)), reshape(mov(:,:,BEGIN_TC:END_TC), [size(mov,1)*size(mov,2) size(mov(:,:,BEGIN_TC:END_TC),3) ])' );

%% Plot correlation figure 
figure
corr_img = reshape(rho', [size(mov,1), size(mov,2)]);
imagesc( corr_img );
axis image;
colorbar;