function groupPdfs

folder = uigetdir; 
cd(folder) 
fileNames = dir('*.pdf'); 

allFileStem = char(regexp(fileNames(1).name,'.*(?=roi)','match'));
groupFileName = [folder,allFileStem,'allFigs.pdf'];

saveFileNameArray = struct2cell(fileNames); 
sa
myAppendPdfs(saveFileName,groupFileName)

% Delete old group pdf 
if exist(groupFilename,'file')
    delete(groupFilename)
end

% Append individual pdfs together
append_pdfs(groupFilename,filenames{:})

% Delete individual pdfs
delete(filenames{:})
